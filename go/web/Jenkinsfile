import groovy.src.jenkins.*
def call(body) {
    def config = [:]
    def projectInfo = [:]

    Docker = null

    try {
        node("jenkins-slave") {
            stage ("test") {
                docker = new Docker(this)
                docker.hello("test message")
            }
        }
    } catch (err) {
        echo "Caught: ${err}"
        currentBuild.result = 'FAILURE'
    } finally {
    }
}