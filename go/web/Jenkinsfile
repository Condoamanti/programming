import groovy.src.jenkins.*
def call(body) {
    def config = [:]
    def projectInfo = [:]

    Docker = null

    try {
        node("jenkins-slave") {
            stage ("test") {
                docker = new Docker(this)
                docker.hello("test message")
            }
        }
    } catch (error) {
        echo "Caught: ${error}"
        currentBuild.result = 'FAILURE'
    }
}